                        <Insets />
                    </HBox.margin>
                    <children>
                        <ComboBox fx:id="comboBox" onAction="#comboBoxWasUpdated" prefHeight="27.0" prefWidth="190.0">
                            <VBox.margin>
                                <Insets left="50.0" />
                            </VBox.margin>
                        </ComboBox>
